class Bufferinstance variablesdata : [nat] := niloperationspublic Buffer: () ==> BufferBuffer() == 	data := nil;public Write: nat ==> ()Write(newData) ==  data := newData;public Read: () ==> natRead() ==    return data;public IsFinished: () ==> ()IsFinished() == skip;syncper Write => #fin(Read) = #fin(Write) + 1;per Read => #fin(Read) = #fin(Write) and not data = nil;per IsFinished => #fin(Read) = 10;end Buffer